fun square(a : int) -> int {
    return a * a;
}

fun factorial(n : int) -> int {
	if(!(n > 1)) {
	// comment
		return 1;
	}
	
	return n * factorial(n - 1);
}

for (var i = 0; i < 10; i++) {
