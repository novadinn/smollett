fun square(a : int) -> int {
    return a * a;
}

fun factorial(n : int) -> int {
	if(n > 1) {
    	 return n * factorial(n - 1);
	}
  	else {
    	 return 1;
	}
}

fun odd(n : int) -> int {
     if(n == 1) {
     	  return 1;
     }
     else {
      return odd(n - 1);
     }

     
}

var a = 15;
var b = a + square(a);
var c = factorial(5);
var d = odd(5);